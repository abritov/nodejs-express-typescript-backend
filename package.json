{
  "name": "typescript",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
		"start": "npm run migrate-dev && yarn run ts-node-dev src/server.ts",
		"start-prod": "yarn run ts-node-dev src/server.ts",
		"migrate": "./node_modules/.bin/sequelize --config ./src/db/config.js --models-path ./src/db/models --seeders-path ./src/db/seeders --migrations-path ./src/db/migrations db:migrate",
		"migrate-prod": "cross-env NODE_ENV=production npm run migrate",
		"migrate-dev": "cross-env NODE_ENV=development npm run migrate",
    "gen-spec-v1": "(cd src/controllers/v1/ && yarn run tinyspec -j -o src/controllers/v1/); yarn run sw2dts ./src/controllers/v1/openapi.json -o ./src/controllers/v1/schema.d.ts"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/passport": "^0.4.7",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/passport-vkontakte": "^1.3.0",
    "@types/sequelize": "^4.27.28",
		"@types/swagger-ui-express": "^3.0.0",
		"cross-env": "^5.2.0",
    "express": "^4.16.4",
    "mysql2": "^1.6.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-vkontakte": "^0.3.2",
    "sequelize": "^4.39.0",
    "sequelize-cli": "^5.1.0",
    "sequelize-serialize": "^1.0.2",
    "swagger-ui-express": "^4.0.1",
    "tinyspec": "^2.0.0",
    "tslib": "^1.9.3",
    "typescript": "^3.1.3"
  },
  "devDependencies": {
    "dotenv": "^6.1.0",
    "sqlite3": "^4.0.3",
    "sw2dts": "^2.6.1",
    "ts-node-dev": "^1.0.0-pre.30"
  }
}
